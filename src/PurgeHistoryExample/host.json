{
  "version": "2.0",
  "extensions": {
    "durableTask": {
      "hubName": "PurgeHistoryExample"
    },
    "tracing": {
      "distributedTracingEnabled": true,
      "distributedTracingProtocol": "W3CTraceContext"
    }
  },
  "logging": {
    "logLevel": {
      "default": "Debug",
      "PurgeHistoryExample.PurgeHistoryTimer": "Debug",
      "PurgeHistoryExample.Function1": "Debug"
    },
    "applicationInsights": {
        "samplingSettings": {
          "isEnabled": true,
          "excludedTypes": "Request"
        },
        "enableLiveMetricsFilters": true
      }
    }
  }